        <Attribute metaattribute.name="Policy Name" metaattribute.id="9A3B52275D1C27C5">81 For new business initiatives supervisors may require that banks implementation</Attribute>
        <Attribute metaattribute.name="Policy Code" metaattribute.id="6F9F70A85D1C4D84">81</Attribute>
        <Attribute metaattribute.name="Absolute Identifier" metaattribute.id="0000000040000003">286D44E55D6D5EFF</Attribute>
        <Attribute metaattribute.name="Name" metaattribute.id="0000000040000002"><Value format="internal">286D44E55D6D5EFF</Value><Value format="display">81 For new business initiatives supervisors may require that banks implementation</Value></Attribute>
        <Attribute metaattribute.name="Create Version" metaattribute.id="0000000040000045"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
        <Attribute metaattribute.name="Update Version" metaattribute.id="0000000040000046"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
        <Attribute metaattribute.name="Modifier" metaattribute.id="0000000040000025"><Value format="internal">CC5FCC8E5069584C</Value><Value format="display">Mega</Value></Attribute>
        <Attribute metaattribute.name="Writing access area" metaattribute.id="0000000040000027"><Value format="internal">0000000033333333</Value><Value format="display">Administrator</Value></Attribute>
        <Attribute metaattribute.name="Comment" metaattribute.id="0000000040000029"><Value textformat="88D68158C658330D">For new business initiatives, supervisors may require that banks&amp;#8217; implementation plans ensure that robust risk data aggregation is possible before allowing a new business venture or acquisition to proceed.&amp;nbsp;</Value><Value textformat="88D68158C658330D" language.name="Français" language.id="5DC05E192E990DA3">Concernant les nouvelles initiatives, les autorit&amp;#233;s de contr&amp;#244;le peuvent exiger des banques qu&amp;#8217;elles garantissent dans leurs plans de mise en &amp;#339;uvre la faisabilit&amp;#233; d&amp;#8217;une agr&amp;#233;gation rigoureuse des donn&amp;#233;es de risque avant d&amp;#8217;autoriser une nouvelle activit&amp;#233; ou acquisition.</Value></Attribute>
        <Attribute metaattribute.name="Creator" metaattribute.id="000000004000003E"><Value format="internal">B8C6BAA137040024</Value><Value format="display">LESAULE Sébastien</Value></Attribute>
        <Attribute metaattribute.name="Creation Date" metaattribute.id="0000000040000005"><Value format="internal">2019/09/02 16:35:49</Value><Value format="display">9/2/2019 6:35:49 PM</Value></Attribute>
        <Attribute metaattribute.name="Modification Date" metaattribute.id="0000000040000006"><Value format="internal">2020/04/29 11:15:00</Value><Value format="display">4/29/2020 1:15:00 PM</Value></Attribute>
