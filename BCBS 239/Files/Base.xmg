        <Attribute metaattribute.name="Name" metaattribute.id="0000000040000002"><Value format="internal">286D411F5D6D58DB</Value><Value format="display">52 Risk management reports should be accurate and precise to ensure a banks board and senior management can rely with confidence on the aggregated information to make critical decisions about risk</Value></Attribute>
        <Attribute metaattribute.name="Create Version" metaattribute.id="0000000040000045"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
        <Attribute metaattribute.name="Update Version" metaattribute.id="0000000040000046"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
