        <Attribute metaattribute.name="Absolute Identifier" metaattribute.id="0000000040000003">286D433F5D6D5D53</Attribute>
        <Attribute metaattribute.name="Name" metaattribute.id="0000000040000002"><Value format="internal">286D433F5D6D5D53</Value><Value format="display">Principle 13 Remedial actions and supervisory measures  Supervisors should have and use the</Value></Attribute>
        <Attribute metaattribute.name="Create Version" metaattribute.id="0000000040000045"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
        <Attribute metaattribute.name="Update Version" metaattribute.id="0000000040000046"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
