        <Attribute metaattribute.name="Policy Name" metaattribute.id="9A3B52275D1C27C5">IV. Supervisory review, tools and cooperation</Attribute>
        <Attribute metaattribute.name="Title" metaattribute.id="63D96F0C5D1B8938"><Value format="internal">Supervisory review, tools and cooperation</Value><Value format="internal" language.name="Français" language.id="5DC05E192E990DA3">Surveillance prudentielle, outils et coopération entre autorités de</Value></Attribute>
        <Attribute metaattribute.name="Policy Code" metaattribute.id="6F9F70A85D1C4D84">IV</Attribute>
        <Attribute metaattribute.name="Absolute Identifier" metaattribute.id="0000000040000003">D97CF8D95D3E4BDD</Attribute>
        <Attribute metaattribute.name="Name" metaattribute.id="0000000040000002"><Value format="internal">D97CF8D95D3E4BDD</Value><Value format="display">IV. Supervisory review, tools and cooperation</Value></Attribute>
        <Attribute metaattribute.name="Create Version" metaattribute.id="0000000040000045"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
        <Attribute metaattribute.name="Update Version" metaattribute.id="0000000040000046"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
        <Attribute metaattribute.name="Modifier" metaattribute.id="0000000040000025"><Value format="internal">CC5FCC8E5069584C</Value><Value format="display">Mega</Value></Attribute>
        <Attribute metaattribute.name="Writing access area" metaattribute.id="0000000040000027"><Value format="internal">0000000033333333</Value><Value format="display">Administrator</Value></Attribute>
        <Attribute metaattribute.name="Comment" metaattribute.id="0000000040000029"><Value textformat="88D68158C658330D">74.&amp;nbsp; Supervisors will have an important role to play in monitoring and providing incentives for a bank&amp;#8217;s implementation of, and ongoing compliance with the Principles. They should also review compliance with the Principles across banks to determine whether the Principles themselves are achieving their desired outcome and whether further enhancements are required.</Value><Value textformat="88D68158C658330D" language.name="Français" language.id="5DC05E192E990DA3">74. Les autorit&amp;#233;s de contr&amp;#244;le auront un r&amp;#244;le important &amp;#224; jouer : elles surveilleront la mise en &amp;#339;uvre des Principes et veilleront &amp;#224; inciter les banques &amp;#224; les appliquer et les respecter en permanence. Elles devraient &amp;#233;galement &amp;#233;valuer la conformit&amp;#233; de l&amp;#8217;ensemble des banques aux Principes pour d&amp;#233;terminer s&amp;#8217;ils permettent d&amp;#8217;obtenir les r&amp;#233;sultats escompt&amp;#233;s et s&amp;#8217;il y a lieu de les am&amp;#233;liorer.</Value></Attribute>
        <Attribute metaattribute.name="Creator" metaattribute.id="000000004000003E"><Value format="internal">B8C6BAA137040024</Value><Value format="display">LESAULE Sébastien</Value></Attribute>
        <Attribute metaattribute.name="Creation Date" metaattribute.id="0000000040000005"><Value format="internal">2019/07/29 13:47:05</Value><Value format="display">7/29/2019 3:47:05 PM</Value></Attribute>
        <Attribute metaattribute.name="Modification Date" metaattribute.id="0000000040000006"><Value format="internal">2020/04/29 11:16:07</Value><Value format="display">4/29/2020 1:16:07 PM</Value></Attribute>
        <Attribute metaattribute.name="Language Update Date" metaattribute.id="0000000040000064"><Value format="internal">2019/10/11 09:30:20</Value><Value format="display">10/11/2019 11:30:20 AM</Value><Value format="internal" language.name="Français" language.id="5DC05E192E990DA3">2019/10/11 08:35:47</Value><Value format="display" language.name="Français" language.id="5DC05E192E990DA3">10/11/2019 10:35:47 AM</Value></Attribute>
