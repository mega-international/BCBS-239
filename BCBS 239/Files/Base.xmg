        <Attribute metaattribute.name="Name" metaattribute.id="0000000040000002"><Value format="internal">286D357C5D6D5414</Value><Value format="display">Principle 6 Adaptability  A bank should be able to generate aggregate risk data to meet a broad range of ondemand ad hoc risk management reporting requests including requests during stresscrisis situations requests due to changing internal needs and requests to meet supervisory queries</Value></Attribute>
        <Attribute metaattribute.name="Create Version" metaattribute.id="0000000040000045"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
