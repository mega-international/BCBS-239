        <Attribute metaattribute.name="Policy Name" metaattribute.id="9A3B52275D1C27C5">Principle 7 Accuracy  Risk management reports should accurately and precisely convey aggregated risk data and reflect risk in an exact manner Reports should be reconciled and validated</Attribute>
        <Attribute metaattribute.name="Policy Code" metaattribute.id="6F9F70A85D1C4D84">Principle 7</Attribute>
        <Attribute metaattribute.name="Absolute Identifier" metaattribute.id="0000000040000003">286D40AE5D6D57EB</Attribute>
        <Attribute metaattribute.name="Name" metaattribute.id="0000000040000002"><Value format="internal">286D40AE5D6D57EB</Value><Value format="display">Principle 7 Accuracy  Risk management reports should accurately and precisely convey aggregated risk data and reflect risk in an exact manner Reports should be reconciled and validated</Value></Attribute>
        <Attribute metaattribute.name="Create Version" metaattribute.id="0000000040000045"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
        <Attribute metaattribute.name="Update Version" metaattribute.id="0000000040000046"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
