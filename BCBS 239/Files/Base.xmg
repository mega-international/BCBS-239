        <Attribute metaattribute.name="Policy Name" metaattribute.id="9A3B52275D1C27C5">41 A banks risk data aggregation capabilities should include all material risk exposures including those that are offbalance sheet</Attribute>
        <Attribute metaattribute.name="Policy Code" metaattribute.id="6F9F70A85D1C4D84">41</Attribute>
        <Attribute metaattribute.name="Absolute Identifier" metaattribute.id="0000000040000003">286D3C455D6D55C0</Attribute>
        <Attribute metaattribute.name="Name" metaattribute.id="0000000040000002"><Value format="internal">286D3C455D6D55C0</Value><Value format="display">41 A banks risk data aggregation capabilities should include all material risk exposures including those that are offbalance sheet</Value></Attribute>
        <Attribute metaattribute.name="Create Version" metaattribute.id="0000000040000045"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
        <Attribute metaattribute.name="Update Version" metaattribute.id="0000000040000046"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
