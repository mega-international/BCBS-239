        <Attribute metaattribute.name="Policy Name" metaattribute.id="9A3B52275D1C27C5">87 National banking supervisors will start discussing implementation of the Principles</Attribute>
        <Attribute metaattribute.name="Policy Code" metaattribute.id="6F9F70A85D1C4D84">87</Attribute>
        <Attribute metaattribute.name="Absolute Identifier" metaattribute.id="0000000040000003">286D46435D6D6075</Attribute>
        <Attribute metaattribute.name="Name" metaattribute.id="0000000040000002"><Value format="internal">286D46435D6D6075</Value><Value format="display">87 National banking supervisors will start discussing implementation of the Principles</Value></Attribute>
        <Attribute metaattribute.name="Create Version" metaattribute.id="0000000040000045"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
        <Attribute metaattribute.name="Update Version" metaattribute.id="0000000040000046"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
        <Attribute metaattribute.name="Modifier" metaattribute.id="0000000040000025"><Value format="internal">CC5FCC8E5069584C</Value><Value format="display">Mega</Value></Attribute>
        <Attribute metaattribute.name="Writing access area" metaattribute.id="0000000040000027"><Value format="internal">0000000033333333</Value><Value format="display">Administrator</Value></Attribute>
        <Attribute metaattribute.name="Comment" metaattribute.id="0000000040000029"><Value textformat="88D68158C658330D">National banking supervisors will start discussing implementation of the Principles with G-SIB&amp;#8217;s senior management in early 2013. This will ensure that banks develop a strategy to meet the Principles by 2016.&amp;nbsp;</Value><Value textformat="88D68158C658330D" language.name="Français" language.id="5DC05E192E990DA3">Les autorit&amp;#233;s nationales de contr&amp;#244;le bancaire commenceront &amp;#224; discuter de la mise en &amp;#339;uvre des Principes avec la direction g&amp;#233;n&amp;#233;rale des EBISm d&amp;#232;s le d&amp;#233;but de 2013, afin que ces &amp;#233;tablissements d&amp;#233;finissent une strat&amp;#233;gie leur permettant de se conformer aux Principes d&amp;#8217;ici &amp;#224; 2016.&amp;nbsp;</Value></Attribute>
        <Attribute metaattribute.name="Creator" metaattribute.id="000000004000003E"><Value format="internal">B8C6BAA137040024</Value><Value format="display">LESAULE Sébastien</Value></Attribute>
        <Attribute metaattribute.name="Creation Date" metaattribute.id="0000000040000005"><Value format="internal">2019/09/02 16:41:39</Value><Value format="display">9/2/2019 6:41:39 PM</Value></Attribute>
