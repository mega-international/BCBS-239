        <Attribute metaattribute.name="Policy Name" metaattribute.id="9A3B52275D1C27C5">63 Reporting policies and procedures should recognise the differing information needs</Attribute>
        <Attribute metaattribute.name="Policy Code" metaattribute.id="6F9F70A85D1C4D84">63</Attribute>
        <Attribute metaattribute.name="Absolute Identifier" metaattribute.id="0000000040000003">286D422F5D6D5AEF</Attribute>
        <Attribute metaattribute.name="Name" metaattribute.id="0000000040000002"><Value format="internal">286D422F5D6D5AEF</Value><Value format="display">63 Reporting policies and procedures should recognise the differing information needs</Value></Attribute>
