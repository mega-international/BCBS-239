        <Attribute metaattribute.name="Policy Name" metaattribute.id="9A3B52275D1C27C5">69 Supervisors expect a bank to confirm periodically with recipients that the information</Attribute>
        <Attribute metaattribute.name="Policy Code" metaattribute.id="6F9F70A85D1C4D84">69</Attribute>
        <Attribute metaattribute.name="Absolute Identifier" metaattribute.id="0000000040000003">286D425B5D6D5BDF</Attribute>
        <Attribute metaattribute.name="Name" metaattribute.id="0000000040000002"><Value format="internal">286D425B5D6D5BDF</Value><Value format="display">69 Supervisors expect a bank to confirm periodically with recipients that the information</Value></Attribute>
        <Attribute metaattribute.name="Create Version" metaattribute.id="0000000040000045"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
        <Attribute metaattribute.name="Update Version" metaattribute.id="0000000040000046"><Value format="internal">30837</Value><Value format="display">V3</Value></Attribute>
        <Attribute metaattribute.name="Modifier" metaattribute.id="0000000040000025"><Value format="internal">CC5FCC8E5069584C</Value><Value format="display">Mega</Value></Attribute>
        <Attribute metaattribute.name="Writing access area" metaattribute.id="0000000040000027"><Value format="internal">0000000033333333</Value><Value format="display">Administrator</Value></Attribute>
        <Attribute metaattribute.name="Comment" metaattribute.id="0000000040000029"><Value textformat="88D68158C658330D">Supervisors expect a bank to confirm periodically with recipients that the information aggregated and reported is relevant and appropriate, in terms of both amount and quality, to the governance and decision-making process.&amp;nbsp;</Value><Value textformat="88D68158C658330D" language.name="Français" language.id="5DC05E192E990DA3">Les autorit&amp;#233;s de contr&amp;#244;le attendent d&amp;#8217;une banque qu&amp;#8217;elle confirme r&amp;#233;guli&amp;#232;rement aux destinataires que les informations agr&amp;#233;g&amp;#233;es et notifi&amp;#233;es sont pertinentes et adapt&amp;#233;es, tant sur le plan de la quantit&amp;#233; que de la qualit&amp;#233;, aux processus de gouvernance et de prise de d&amp;#233;cision.&amp;nbsp;</Value></Attribute>
        <Attribute metaattribute.name="Creator" metaattribute.id="000000004000003E"><Value format="internal">B8C6BAA137040024</Value><Value format="display">LESAULE Sébastien</Value></Attribute>
