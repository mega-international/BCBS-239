        <Attribute metaattribute.name="Language Update Date" metaattribute.id="0000000040000064"><Value format="internal">2019/10/11 09:34:46</Value><Value format="display">10/11/2019 11:34:46 AM</Value><Value format="internal" language.name="Français" language.id="5DC05E192E990DA3">2019/10/11 08:37:00</Value><Value format="display" language.name="Français" language.id="5DC05E192E990DA3">10/11/2019 10:37:00 AM</Value></Attribute>
