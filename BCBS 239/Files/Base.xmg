        <Attribute metaattribute.name="Policy Name" metaattribute.id="9A3B52275D1C27C5">Principle 11 Distribution  Risk management reports should be distributed to the relevant parties</Attribute>
        <Attribute metaattribute.name="Policy Code" metaattribute.id="6F9F70A85D1C4D84">Principle 11</Attribute>
        <Attribute metaattribute.name="Absolute Identifier" metaattribute.id="0000000040000003">286D40DC5D6D588B</Attribute>
        <Attribute metaattribute.name="Name" metaattribute.id="0000000040000002"><Value format="internal">286D40DC5D6D588B</Value><Value format="display">Principle 11 Distribution  Risk management reports should be distributed to the relevant parties</Value></Attribute>
